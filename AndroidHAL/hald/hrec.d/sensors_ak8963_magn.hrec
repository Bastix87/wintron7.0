# AK8963 Magnetometer HAL record

[global]
module_name = sensors
library_name = iio-sensors-hal.so

[device]
bus = acpi
desc = AK896300

[properties]
ro.iio.magn.name = AK8963 Compass
ro.iio.magn.vendor = Asahi Kasei
ro.iio.magn.max_range = 9800
ro.iio.magn.max_freq = 20
ro.iio.magn.resolution = 0.06
ro.iio.magn.power = 0.28
ro.iio.magn.filter = average,20

[fuse]
src_name = android.hardware.sensor.compass.xml
mount_type = permissions
